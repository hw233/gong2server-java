package com.gamejelly.gong2.config.data;

import com.gamejelly.gong2.config.data.base.LData;
import com.gamejelly.gong2.config.data.base.LMap;

/**
 * 动物表.csv
 * 编号 = id
 * 所属建筑编号 = buildId
 * 判断是否spine动画 = isSpine
 * spine文件名 = filename
 * 缩放 = suofang
 */
public class AnimalActionData extends LData {
    public static final LMap data = new LMap();

    static {
        init0();
        init1();
        init2();
        data.setFrozen(true);
    }

    private static void init0() {
data.map(
    100000, map("id", 100000, "isSpine", true, "filename", "cat_01", "suofang", 0.5f),
    100001, map("id", 100001, "isSpine", true, "filename", "dog01", "suofang", 0.5f),
    100002, map("id", 100002, "isSpine", true, "filename", "pony01"),
    100003, map("id", 100003),
    100004, map("id", 100004, "isSpine", true, "filename", "hudie01")
);
    }

    private static void init1() {
data.map(
    100005, map("id", 100005, "isSpine", true, "filename", "bee"),
    100006, map("id", 100006, "isSpine", true, "filename", "frog", "suofang", 0.3f),
    100007, map("id", 100007, "isSpine", true, "filename", "bird"),
    100008, map("id", 100008, "isSpine", true, "filename", "dog02", "suofang", 0.5f),
    100009, map("id", 100009, "isSpine", true, "filename", "hudie02")
);
    }

    private static void init2() {
data.map(
    100010, map("id", 100010, "isSpine", true, "filename", "pony02"),
    100011, map("id", 100011, "isSpine", true, "filename", "cat02", "suofang", 0.5f),
    100012, map("id", 100012, "isSpine", true, "filename", "pony03")
);
    }

}
package com.gamejelly.gong2.meta;

import com.gamejelly.gong2.meta.base.ChildModel;
import com.hadoit.game.common.framework.dao.annotation.Table;

/**
 * 申请消息
 * @author apple
 *
 */
@Table(recursion = true)
public class GongHuiApplyModel extends ChildModel {

    private static final long serialVersionUID = 1L;

    /**
     * 申请人ID
     */
    private String fromAvatarId;

    /**
     * 申请人名字
     */
    private String fromName;

    private int fromLv;

    private int fromGrade;

    private int fromSex;

    private long createTime;

    public GongHuiApplyModel() {
    }

    public String getFromAvatarId() {
        return fromAvatarId;
    }

    public void setFromAvatarId(String fromAvatarId) {
        this.fromAvatarId = fromAvatarId;
    }

    public String getFromName() {
        return fromName;
    }

    public void setFromName(String fromName) {
        this.fromName = fromName;
    }

    public int getFromLv() {
        return fromLv;
    }

    public void setFromLv(int fromLv) {
        this.fromLv = fromLv;
    }

    public int getFromGrade() {
        return fromGrade;
    }

    public void setFromGrade(int fromGrade) {
        this.fromGrade = fromGrade;
    }

    public int getFromSex() {
        return fromSex;
    }

    public void setFromSex(int fromSex) {
        this.fromSex = fromSex;
    }

    public long getCreateTime() {
        return createTime;
    }

    public void setCreateTime(long createTime) {
        this.createTime = createTime;
    }

}

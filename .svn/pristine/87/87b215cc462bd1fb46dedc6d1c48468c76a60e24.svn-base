<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean id="dbsManager" class="com.hadoit.game.engine.guardian.dbs.DbsManager"
		destroy-method="dispose">
		<property name="workThreads" value="${config.dbs_work_threads}" />
		<property name="listenHost" value="${config.dbs_host}" />
		<property name="listenPort" value="${config.dbs_port}" />
		<property name="serverId" value="${config.server_id}" />
		<property name="codecFactory">
			<bean
				class="com.hadoit.game.engine.core.rpc.base.codec.factory.FstMessageCodecFactory" />
		</property>
		<property name="dbsServerProxy" ref="dbsServerProxySql" />
	</bean>
</beans>
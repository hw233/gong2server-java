}
.bigBtn {
	PADDING-RIGHT: 18px; PADDING-LEFT: 18px; PADDING-BOTTOM: 0px; PADDING-TOP: 0px; HEIGHT: 28px
}
.err {
	PADDING-RIGHT: 0px; PADDING-LEFT: 20px; BACKGROUND: url(i/err.png) no-repeat 2px 0px; PADDING-BOTTOM: 0px; COLOR: #f30; PADDING-TOP: 0px
}
.pageIndex {
	TEXT-ALIGN: center
}
.tips {
	DISPLAY: block; MARGIN: 0px 3px; COLOR: #a88
}
#wrapper {
	MARGIN: 0px
}
#header {
	PADDING-RIGHT: 5px; PADDING-LEFT: 5px; BACKGROUND: #369; PADDING-BOTTOM: 5px; COLOR: #fff; PADDING-TOP: 5px; POSITION: relative; HEIGHT: 22px
}
#memu {
	FLOAT: left; WIDTH: 170px
}
#mContent {
	MARGIN: 0px 0px 0px 180px
}
#mContent .title {
	BORDER-RIGHT: #ccc 1px solid; PADDING-RIGHT: 2px; BORDER-TOP: #ccc 1px solid; PADDING-LEFT: 10px; BACKGROUND: #eee; PADDING-BOTTOM: 2px; MARGIN: 4px 4px 0px; FONT: bold 14px/26px arial,'宋体'; BORDER-LEFT: #ccc 1px solid; PADDING-TOP: 2px; BORDER-BOTTOM: #ccc 1px solid
}
#mContent .buttons {
	PADDING-RIGHT: 10px; PADDING-LEFT: 0px; FLOAT: right; PADDING-BOTTOM: 0px; PADDING-TOP: 0px
}
.searchPanel {
	BORDER-RIGHT: #ccc 1px solid; PADDING-RIGHT: 10px; BORDER-TOP: #ccc 1px solid; PADDING-LEFT: 10px; BACKGROUND: url(i/search_bar.gif) repeat-x; PADDING-BOTTOM: 5px; MARGIN: 4px; BORDER-LEFT: #ccc 1px solid; PADDING-TOP: 5px; BORDER-BOTTOM: #ccc 1px solid
}
.tableWrapper {
	BORDER-RIGHT: #ccc 1px solid; PADDING-RIGHT: 1px; BORDER-TOP: #ccc 1px solid; PADDING-LEFT: 1px; BACKGROUND: #eee; PADDING-BOTTOM: 1px; MARGIN: 4px; BORDER-LEFT: #ccc 1px solid; PADDING-TOP: 1px; BORDER-BOTTOM: #ccc 1px solid
}
.grid {
	BACKGROUND: #fff; WIDTH: 100%; border-spacing: 1px
}
.gridview {
	BACKGROUND: #fff; WIDTH: 100%; border-spacing: 1px
}
.grid {
	WIDTH: 99.6%
}
.gridview {
	WIDTH: 99.6%
}
.grid TD {
	PADDING-RIGHT: 2px; PADDING-LEFT: 2px; PADDING-BOTTOM: 2px; PADDING-TOP: 2px
}
.gridview TD {
	PADDING-RIGHT: 2px; PADDING-LEFT: 2px; PADDING-BOTTOM: 2px; PADDING-TOP: 2px
}
.grid THEAD TD {
	BACKGROUND: #eef; FONT: bold 12px/20px arial,'宋体'
}
.gridview THEAD TD {
	BACKGROUND: #eef; FONT: bold 12px/20px arial,'宋体'
}
.grid THEAD TH {
	BACKGROUND: #eef; FONT: bold 12px/20px arial,'宋体'
}
.gridview THEAD TH {
	BACKGROUND: #eef; FONT: bold 12px/20px arial,'宋体'
}
.grid TFOOT TD {
	PADDING-RIGHT: 0px; PADDING-LEFT: 0px; BACKGROUND: #eef; PADDING-BOTTOM: 2px; PADDING-TOP: 3px
}
.gridview TFOOT TD {
	PADDING-RIGHT: 0px; PADDING-LEFT: 0px; BACKGROUND: #eef; PADDING-BOTTOM: 2px; PADDING-TOP: 3px
}
.grid .label {
	PADDING-RIGHT: 2px; PADDING-LEFT: 2px; BACKGROUND: #eee; PADDING-BOTTOM: 2px; WIDTH: 100px; PADDING-TOP: 2px; TEXT-ALIGN: right
}
.gridview .label {
	PADDING-RIGHT: 2px; PADDING-LEFT: 2px; BACKGROUND: #eee; PADDING-BOTTOM: 2px; WIDTH: 100px; PADDING-TOP: 2px; TEXT-ALIGN: right
}
.tabsort TH {
	CURSOR: pointer; TEXT-DECORATION: underline
}
.tabsort TH.headerSortDown {
	COLOR: saddlebrown
}
.tabsort TH.headerSortUp {
	COLOR: #4297d7
}
.pageIndex {
	PADDING-RIGHT: 10px; PADDING-LEFT: 2px; FLOAT: right; PADDING-BOTTOM: 2px; FONT: bold 12px georgia; COLOR: #666; PADDING-TOP: 2px; TEXT-ALIGN: center
}
.pageIndex SPAN {
	MARGIN: 0px 1px
}
.pageIndex A {
	BORDER-RIGHT: #bbb 1px solid; PADDING-RIGHT: 5px; BORDER-TOP: #bbb 1px solid; PADDING-LEFT: 5px; PADDING-BOTTOM: 1px; MARGIN: 0px 1px; BORDER-LEFT: #bbb 1px solid; COLOR: #99f; PADDING-TOP: 1px; BORDER-BOTTOM: #bbb 1px solid; TEXT-DECORATION: none
}
.pageIndex A:hover {
	BORDER-LEFT-COLOR: #c60; BACKGROUND: #ebecd2; BORDER-BOTTOM-COLOR: #c60; BORDER-TOP-COLOR: #c60; BORDER-RIGHT-COLOR: #c60
}
.pageIndex SPAN.c {
	BORDER-RIGHT: #f96 1px solid; PADDING-RIGHT: 5px; BORDER-TOP: #f96 1px solid; PADDING-LEFT: 5px; FONT-WEIGHT: bold; BACKGROUND: #fda; PADDING-BOTTOM: 2px; BORDER-LEFT: #f96 1px solid; COLOR: #f60; PADDING-TOP: 2px; BORDER-BOTTOM: #f96 1px solid
}
.pageIndex SPAN.d {
	BORDER-RIGHT: #ddd 1px solid; PADDING-RIGHT: 5px; BORDER-TOP: #ddd 1px solid; PADDING-LEFT: 5px; PADDING-BOTTOM: 2px; BORDER-LEFT: #ddd 1px solid; COLOR: #aaa; PADDING-TOP: 2px; BORDER-BOTTOM: #ddd 1px solid
}
.menu {
	MARGIN: 0px 0px 0px 4px; WIDTH: 180px
}
.menu LI {
	WIDTH: 174px
}
.menu A {
	PADDING-RIGHT: 0px; DISPLAY: block; PADDING-LEFT: 0px; PADDING-BOTTOM: 0px; MARGIN: 1px; FONT: 12px/20px arial,"宋体"; COLOR: #369; PADDING-TOP: 0px
}
.menu .hlight {
	FONT-WEIGHT: bold
}
.menu UL A {
	PADDING-LEFT: 25px; BACKGROUND: url(i/cate.gif) #dbeaf5 no-repeat 12px 50%
}
.menu UL UL A {
	PADDING-LEFT: 40px; BACKGROUND: url(i/cate.gif) #f0f8fe no-repeat 27px 50%
}
.menu UL UL UL A {
	PADDING-LEFT: 55px; BACKGROUND: url(i/cate.gif) #f0f8fe no-repeat 42px 50%
}
.menu UL UL UL UL A {
	PADDING-LEFT: 70px; BACKGROUND: url(i/cate.gif) #f9f9f9 no-repeat 67px 50%
}
.menu UL A:hover {
	BACKGROUND: #dfd
}
.menu .main {
	PADDING-RIGHT: 0px; PADDING-LEFT: 4px; BACKGROUND: url(i/nav_l.gif) no-repeat left top; PADDING-BOTTOM: 0px; PADDING-TOP: 0px; TEXT-DECORATION: none
}
.menu .main SPAN {
	PADDING-RIGHT: 12px; DISPLAY: block; PADDING-LEFT: 6px; BACKGROUND: url(i/nav_r.gif) no-repeat right top; PADDING-BOTTOM: 4px; FONT: bold 12px arial,"宋体"; COLOR: #fff; PADDING-TOP: 6px
}
.menu .main SPAN {
	FLOAT: none
}
.menu .main:hover SPAN {
	COLOR: #fff
}
.menu .main:hover {
	BACKGROUND-POSITION: 0% -42px
}
.menu .main:hover SPAN {
	BACKGROUND-POSITION: 100% -42px
}
.tc {
	TEXT-ALIGN: center
}
#menu TABLE {
	FONT-SIZE: 12px; LINE-HEIGHT: 150%; FONT-FAMILY: "Verdana","Arial","Helvetica","sans-serif"
}
A.menuChild {
	FONT-SIZE: 12px; LINE-HEIGHT: 150%; FONT-FAMILY: "Verdana","Arial","Helvetica","sans-serif"
}
A.menuParent {
	FONT-SIZE: 12px; LINE-HEIGHT: 150%; FONT-FAMILY: "Verdana","Arial","Helvetica","sans-serif"
}
A.menuChild:link {
	COLOR: #036; TEXT-DECORATION: none
}
A.menuChild:visited {
	COLOR: #036; TEXT-DECORATION: none
}
A.menuChild:hover {
	COLOR: #f60; TEXT-DECORATION: underline
}
A.menuParent:link {
	COLOR: #000; TEXT-DECORATION: none
}
A.menuParent:visited {
	COLOR: #000; TEXT-DECORATION: none
}
A.menuParent:hover {
	COLOR: #f60; TEXT-DECORATION: none
}
TD .menuPtd {

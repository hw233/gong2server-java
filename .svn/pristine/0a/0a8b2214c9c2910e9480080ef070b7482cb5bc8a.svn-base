package com.gamejelly.gong2.config.data;

import com.gamejelly.gong2.config.data.base.LData;
import com.gamejelly.gong2.config.data.base.LMap;

/**
 * 属性转换数值表.csv
 * 装备编号 = id
 * 倍率 = bl
 */
public class PropNumData extends LData {
    public static final LMap data = new LMap();

    static {
        init0();
        init1();
        data.setFrozen(true);
    }

    private static void init0() {
data.map(
    20003, map("id", 20003, "bl", 14.0f),
    21003, map("id", 21003, "bl", 5.833333333f),
    22003, map("id", 22003, "bl", 8.666666667f),
    23003, map("id", 23003, "bl", 4.166666667f),
    24003, map("id", 24003, "bl", 3.5f)
);
    }

    private static void init1() {
data.map(
    25003, map("id", 25003, "bl", 2.5f),
    26003, map("id", 26003, "bl", 1.0f)
);
    }

}